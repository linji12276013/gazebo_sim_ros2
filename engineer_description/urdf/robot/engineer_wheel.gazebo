<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
  
  <xacro:macro name="engineer_wheel_gazebo" params="prefix params">
    <gazebo reference="wheel_${prefix}">
      <selfCollide>true</selfCollide>
      <gravity>true</gravity>
      <collision>
        <surface>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
              <fdir1>${params.fdir1}</fdir1>
            </ode>
          </friction>
        </surface>
      </collision>
      <kp>1e50</kp>
      <kd>1e50</kd>
      <material>Gazebo/White</material>
    </gazebo>
  </xacro:macro>
</robot>
